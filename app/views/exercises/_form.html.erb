<%= form_for @exercise do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :required => true %>

  <%= f.label :equipment %>
  <%= f.text_field :equipment %>

  <%= f.label :body_part %>
  <%= f.text_field :body_part%>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :image %>
  <%= f.url_field :image %>

  <%= f.label :video %>
  <%= f.url_field :video %>

  <%= f.submit %>
<% end %>
